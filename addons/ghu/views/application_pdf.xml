<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <template id="application_agreement">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h2>Application for doctoral program</h2>
                            <div class="container mt-2">
                                <div class="row">
                                    <div class="row">
                                        <div class="form-field form-group col-md-6">
                                            <div class="row">
                                                <label for="first_name" class="control-label col-sm-3" title="">
                                                    First Name
                                                </label>
                                                <div class="col-sm-9">
                                                    <span t-field="o.first_name"/>
                                                </div>

                                            </div>
                                        </div>
                                        <div class="form-field form-group col-md-6">
                                            <div class="row">
                                                <label for="last_name" class="control-label col-sm-3" title="">
                                                    Last Name
                                                </label>
                                                <div class="col-sm-9">
                                                    <span t-field="o.last_name"/>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>