<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <template id="assets_frontend" name="Campus Assets" inherit_id="website.assets_frontend">
            <xpath expr="link" position="replace">
                <!-- Perfect Scrollbar -->
                <link type="text/css" href="/theme_campus/static/design/dist/assets/vendor/perfect-scrollbar.css" rel="stylesheet" />

                <!-- Material Design Icons -->
                <link type="text/css" href="/theme_campus/static/design/dist/assets/css/material-icons.css" rel="stylesheet" />

                <!-- Font Awesome Icons -->
                <link type="text/css" href="/theme_campus/static/design/dist/assets/css/fontawesome.css" rel="stylesheet" />

                <!-- App CSS -->
                <link type="text/css" href="/theme_campus/static/design/dist/assets/css/app.css" rel="stylesheet" />
            </xpath>
            
            <xpath expr="script[last()]" position="after">
                <script src="/theme_campus/static/design/dist/assets/vendor/jquery.min.js"></script>

                <!-- Bootstrap -->
                <script src="/theme_campus/static/design/dist/assets/vendor/popper.min.js"></script>
                <script src="/theme_campus/static/design/dist/assets/vendor/bootstrap.min.js"></script>

                <!-- Perfect Scrollbar -->
                <script src="/theme_campus/static/design/dist/assets/vendor/perfect-scrollbar.min.js"></script>

                <!-- MDK -->
                <script src="/theme_campus/static/design/dist/assets/vendor/dom-factory.js"></script>
                <script src="/theme_campus/static/design/dist/assets/vendor/material-design-kit.js"></script>

                <!-- App JS -->
                <script src="/theme_campus/static/design/dist/assets/js/app.js"></script>

                <!-- Highlight.js -->
                <script src="/theme_campus/static/design/dist/assets/js/hljs.js"></script>
            </xpath>
        </template>
    </data>
</odoo>