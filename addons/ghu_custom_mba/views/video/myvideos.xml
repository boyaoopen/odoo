<odoo>
    <data noupdate="0">
        <template id="myvideos">
            <t t-call="ghu_custom_mba.campus_layout">
                <t t-set="title">
                    My video folder
                </t>
                <t t-set="subtitle">
                    Manage private videos
                </t>
                <iframe height="0" width="0" id="FileFrame" src="about:blank"></iframe>
                <script type="text/javascript">
                var doc = document.getElementById('FileFrame').contentWindow.document;
                doc.open();doc.write('<html><head><title></title></head><body><form name="ltiLaunchForm" method="post" t-attf-action="#{bltiParams['launch_url']}"><t t-foreach="bltiParams['launch_data']" t-as="param"><input type="hidden" t-attf-name="{{ param }}" t-attf-value="{{ param_value }}" /></t></form><script language="javascript">document.ltiLaunchForm.submit();</script></body></html>');doc.close();
                </script>
            </t>
        </template>
    </data>
</odoo>