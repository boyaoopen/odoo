                'first_name',
                'last_name',
                'title_post',
                'gender',
                'marital_status',
                'birthday',
                'nationality',
                'native_language',
                'foreign_languages',
                'company_position',
                'email',
                'skype',
                'street',
                'street2',
                'zip',
                'city',
                'state_id',
                'country_id',
                'phone',
                'mobile',
                'photo',
                'vita',
                'passport',
                'product_id',
                'study_id'
            ]"/>
        </function>
    </data>
</openerp>